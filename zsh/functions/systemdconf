./autogen.sh
./configure \
    PYTHON=python3 \
    PYTHON_CONFIG=/usr/bin/python3-config \
    --libexecdir=/usr/lib \
    --localstatedir=/var \
    --sysconfdir=/etc \
    --enable-introspection \
    --enable-gtk-doc \
    --disable-audit \
    --disable-ima \
    --with-distro=arch \
    --with-sysvinit-path= \
    --with-sysvrcnd-path=
